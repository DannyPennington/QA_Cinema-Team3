GET        /assets/*file          controllers.Assets.versioned(path="/public", file: Asset)

GET        /                      controllers.HomeController.index

GET        /listingGallery        controllers.GalleryController.listingGallery
GET        /releaseGallery        controllers.GalleryController.releaseGallery
GET        /createMovie           controllers.GalleryController.createMovie
GET        /createRelease         controllers.GalleryController.createRelease